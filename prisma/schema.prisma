generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Token {
  requestToken    String @id
  accessToken     String?
  refreshToken    String?
}

model Trade {
  id             String @id
  tradingsymbol  String
  token          Int
  averagePrice   Float
  quantity       Int
  instrumentType String
  buyOrSell      String
  segment        String
  exchange       String
  tag            String
}
